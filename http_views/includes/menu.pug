//- includes/menu.pug
.header
    h3
        img(src= Config.dndVaultIcon)
        .
            D&amp;D Vault - #{title} #{discordMe?'('+discordMe.username+'#'+discordMe.discriminator+')':''}
    .
        #{guildConfig?'@ '+guildConfig.name:''}
.topnav
    a(href= '/') Support
    //- |&nbsp;|&nbsp;
    a(href= '/timezones') Timezones
    if discordMe
        //- |&nbsp;|&nbsp;
        a(href= '/events') Events
        //- |&nbsp;|&nbsp;
        a(href= '/calendar?userID='+discordMe.id) Calendar ICS
    if guildConfig && guildConfig.botID
        a(href= 'https://top.gg/bot/'+guildConfig.botID+'/vote', target="_blank") Vote For Me on Top.gg!
        //- |&nbsp;|&nbsp;
    if discordMe
        //- |&nbsp;|&nbsp;
        a(href= '/logout') Logout
